{
  "name": "autocentaur_moving_conpany",
  "version": "1.0.0",
  "description": "Autocentaur WebSite",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm start --prefix frontend",
    "client:install": "npm install --prefix frontend",
    "client:build": "npm run build --prefix frontend",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "moving",
    "autocentaur"
  ],
  "author": "Grigorii Pashkov <pashkovgrigorii@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.9",
    "express": "^4.18.2",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  }
}
